    "@shopify/app-bridge-react": "^4.1.5",
    "@shopify/polaris": "^13.8.0",
    "@shopify/polaris-icons": "^9.3.0",
    "gadget-server": "link:.gadget/server",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.0.2",
    "shopify-api-node": "^3.14.2"
  },
  "devDependencies": {
    "@shopify/app-bridge-types": "^0.0.15",
    "@types/node": "^20.17.7",
